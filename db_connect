<?php
$host = "localhost";
$dbname = "fuelbuddy";
$dbusername = "root";  // Default for XAMPP
$dbpassword = "";  // Leave empty if no password

$conn = new mysqli($host, $dbusername, $dbpassword, $dbname);

// Check connection
if ($conn->connect_error) {
    die("Connection failed: " . $conn->connect_error);
}
?>
